{
  "expo": {
    "name": "Chewabl - Quick Dining Picks",
    "slug": "chewabl-quick-dining-picks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rork-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "app.rork.chewabl-quick-dining-picks"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "app.rork.chewabl_quick_dining_picks"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://rork.com/"
        }
      ],
      "expo-font",
      "expo-web-browser",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow Chewabl to use your location to find nearby restaurants."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#FF6B35"
        }
      ],
      [
        "expo-contacts",
        {
          "contactsPermission": "Allow Chewabl to access your contacts to find friends."
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Chewabl to access your photos to set a profile picture."
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}